<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @*@Scripts.Render("~/bundles/modernizr")*@
</head>
<body ng-app="app">
    <nav>
        <ul>
            <li><a href="#/owners">Owners</a></li>
            <li><a href="#/mortgages">mortgages</a></li>
            <li><a href="#/landProperties">Land Properties</a></li>
        </ul>
    </nav>
    <div class="container body-content">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @*@RenderSection("scripts", required: false)*@
    @Scripts.Render("~/bundles/angular-vendor")
    @Scripts.Render("~/bundles/app-modules")
    @Scripts.Render("~/bundles/app-services")
    @Scripts.Render("~/bundles/app-controllers")
    @Scripts.Render("~/bundles/app-routes")
</body>
</html>
